{% extends 'admin/base.html.twig' %}

{% block body_content %}

    {{ parent() }}
    
    <div class="row mb-4">

        <div class="col-lg-12">

            {{ form_start(form) }}

            {{ include('admin/contractor/_form.html.twig') }}

            <div class="float-start">
                <button class="btn btn-success">{{ 'action.update'|trans }}</button>
                <a href="{{ path('app_admin_contractor_index') }}" title="{{ 'action.back_to_list'|trans }}" class="btn btn-secondary">{{ 'action.back_to_list'|trans }}</a>
            </div>

            {{ form_end(form) }}

            {{ include('admin/contractor/_delete_form.html.twig') }}

        </div>

    </div>

{% endblock %}

{% block body_javascript_function %}

    {{ parent() }}

    {{ include('admin/inc/body-javascript-function/contractor.html.twig') }}

{% endblock %}

{% block body_javascript_document_ready %}

    {{ parent() }}
    
    var module = "contractor";
    var moduleUrl = "{{ path('app_admin_supplier_new_ares') }}";
    {{ include('admin/inc/body-javascript-document-ready/ares.html.twig') }}

{% endblock %}
