<form method="post" action="{{ path('app_admin_supplier_contact_delete', {'id': supplierContact.id, 'supplierId': supplierId}) }}" onsubmit="return confirm('{{ 'alert.are_you_sure_you_want_to_delete_this_item'|trans }}');" class="d-inline-block float-end">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ supplierContact.id) }}">
    <button class="btn btn-danger">{{ 'action.delete'|trans }}</button>
</form>
